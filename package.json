{
  "name": "game-base-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --build-optimizer=true --aot=true --extract-css=true --named-chunks=false --vendor-chunk=true --base-href https://dev.game-base.de",
    "build-live": "ng build --prod --build-optimizer=true --aot=true --extract-css=true --named-chunks=false --vendor-chunk=true --base-href https://game-base.de",
    "test": "ng test --code-coverage --progress false --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "openapi": "ng-openapi-gen --input ./vendor/swagger-rest-api/yaml-resolved/gamebase-api-specification.yaml --output ./src/app/rest-client",
    "preinstall": "npx npm-force-resolutions",
    "sonarqube": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@nebular/auth": "^5.1.0",
    "@nebular/eva-icons": "^5.1.0",
    "@nebular/theme": "^5.1.0",
    "echarts": "^4.8.0",
    "eva-icons": "^1.1.3",
    "ngx-echarts": "^5.0.0",
    "ngx-toastr": "^12.1.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.13",
    "chai": "^4.2.0",
    "codelyzer": "^5.2.2",
    "cucumber": "^6.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "minimist": "^1.2.5",
    "ng-openapi-gen": "^0.12.1",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "sonarqube-scanner": "^2.6.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
